{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "local": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "credentialsSource": "remote",
      "ios": {
        "resourceClass": "default"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
